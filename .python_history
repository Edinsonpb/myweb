num1 = 1.99
num2 = 2.01
sum = num1 + num2
print('El resultado de la suma de {0} y {1} es : {sum}')
print('El resultado de la suma de {0} y {1} es : {3}'. format(num1, num2, sum) )
print('El resultado de la suma de {0} y {1} es : {3}'. format(num1, num2, sum))
print('El resultado de la suma de {0} y {1} es : {2}'. format(num1, num2, sum) )
